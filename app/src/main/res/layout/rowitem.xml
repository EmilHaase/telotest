<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginRight="2dp"
    android:layout_marginLeft="2dp"
    android:foreground="@drawable/card_foreground"
    card_view:cardBackgroundColor="@color/colorCardBackground"
    card_view:cardCornerRadius="2dp"
    card_view:cardElevation="3dp"
    card_view:cardPreventCornerOverlap="false"
    card_view:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="8">



            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="@dimen/thumbSize"
                android:layout_height="@dimen/thumbSize"
                android:layout_margin="5dp"
                android:gravity="center_vertical"
                android:layout_gravity="center"
                android:scaleType="center"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/cloudIcon"
                android:layout_width="@dimen/thumbSize"
                android:layout_height="@dimen/thumbSize"
                android:layout_margin="5dp"
                android:gravity="center_vertical"
                android:layout_gravity="center"
                android:scaleType="center"
                android:src="@drawable/baseline_cloud_done_24"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/iconID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_margin="5dp"
                android:scaleType="center"
                android:layout_weight="1"
                android:src="@drawable/baseline_insert_drive_file_24"
                app:tint="#14532D"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="3"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:id="@+id/id_textView_card_TrackName"
                            android:textColor="@color/textColorCardTitle"
                            android:textStyle="bold"
                            android:textDirection="locale"
                            android:gravity="start"
                            android:layout_margin="1dp"
                            android:textAlignment="viewStart" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:id="@+id/id_textView_card_TrackDesc"
                            android:maxLines="1"
                            android:ellipsize="end"
                            android:textStyle="italic"
                            android:textDirection="locale"
                            android:gravity="start"
                            android:textAlignment="viewStart"
                            android:layout_margin="1dp"
                            android:layout_below="@id/id_textView_card_TrackName"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/id_device_type_rowitem"
                        android:text="M"
                        android:autoSizeTextType="uniform"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:maxLines="1">
                        
                    </TextView>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.75"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="from"  />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="Events" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="minT1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="minHum" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_file_from"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_file_count"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_min_tx"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_min_hum"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="into"  />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="bytes" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="maxT1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:text="maxHum" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_file_into"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_file_size"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_max_tx"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="1dp">
                        <TextView
                            android:id="@+id/id_max_hum"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="@style/TextAppearanceCardSmall"
                            android:textColor="@color/textColorPrimary"
                            android:maxLines="1"
                            android:gravity="center"
                            android:text=""
                            android:autoSizeTextType="uniform" />
                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>


</androidx.cardview.widget.CardView>